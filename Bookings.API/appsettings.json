{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Bookings": "Host=localhost;Database=bookings;Username=postgres;Password=postgres",
    "Transponder": "Host=localhost;Database=transponder;Username=postgres;Password=postgres",
    "Redis": "rediss://localhost:6379"
  },
  "TransponderSettings": {
    "GrpcPortOffset": 0
  },
  "TransponderDefaults": {
    "LocalAddress": "https://localhost:7266",
    "RemoteAddress": "https://localhost:7268"
  },
  "TransponderPersistence": {
    "Schema": "bookings_transponder"
  }
}
